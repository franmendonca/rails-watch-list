<div class="row justify-content-center">
  <% @lists.each do |list| %>
    <div class="card col-4">
      <%= image_tag("theater.jpg", class: "card-img-top", alt: "list placeholder", size:"300x300") %>
      <div class="card-body">
        <h5 class="card-title"><%= list.name %></h5>
        <td><%= link_to 'Show', list, class: "btn btn-secondary" %></td>
        <td><%=link_to 'New Bookmark', new_list_bookmark_path(list), class: "btn btn-success"%></td>
      </div>
    </div>
  <% end %>
</div>
